    <script>
        // Navbar scroll effect
        window.addEventListener('scroll', function() {
            const navbar = document.getElementById('navbar');
            if (window.scrollY > 100) {
                navbar.classList.add('scrolled');
            } else {
                navbar.classList.remove('scrolled');
            }
        });

        // Certification chart interactivity
        document.addEventListener('DOMContentLoaded', function() {
            const certDataPoints = document.querySelectorAll('.cert-data-point');

            certDataPoints.forEach(point => {
                point.addEventListener('mouseenter', function() {
                    const certNum = this.getAttribute('data-cert');
                    const tooltip = document.querySelector(`.cert-tooltip[data-cert="${certNum}"]`);
                    if (tooltip) {
                        tooltip.style.opacity = '1';
                        tooltip.style.visibility = 'visible';
                        tooltip.style.transform = 'translate(-50%, -10px)';
                    }
                });

                point.addEventListener('mouseleave', function() {
                    const certNum = this.getAttribute('data-cert');
                    const tooltip = document.querySelector(`.cert-tooltip[data-cert="${certNum}"]`);
                    if (tooltip) {
                        tooltip.style.opacity = '0';
                        tooltip.style.visibility = 'hidden';
                        tooltip.style.transform = 'translate(-50%, 0px)';
                    }
                });
            });
        });
    </script>

    <style>
        @keyframes chartPulse {
            0% { transform: scale(1); }
            50% { transform: scale(1.08); }
            100% { transform: scale(1); }
        }
